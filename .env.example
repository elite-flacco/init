# AI Configuration
# Set the AI provider: 'openai', 'anthropic', or 'mock'
VITE_AI_PROVIDER=mock

# OpenAI Configuration (uncomment and add your key to use OpenAI)
# VITE_OPENAI_API_KEY=sk-your-openai-api-key-here

# Anthropic Configuration (uncomment and add your key to use Anthropic)
# VITE_ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here

# AI Model Configuration
VITE_AI_MODEL=gpt-4
VITE_AI_MAX_TOKENS=1000
VITE_AI_TEMPERATURE=0.7

# Optional: Custom AI API Base URL (for self-hosted or proxy endpoints)
# VITE_AI_BASE_URL=https://your-custom-api-endpoint.com